<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayoutFhome"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentHome">

    <com.example.fitness.CircleProgressBar
        android:id="@+id/steps_progressBar"
        android:layout_width="@dimen/progressBar_dimen"
        android:layout_height="@dimen/progressBar_dimen_height"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:progress="35"
        app:progressBarThickness="4dp" />

    <com.example.fitness.CircleProgressBar
        android:id="@+id/water_progressBar"
        android:layout_width="@dimen/progressBar_dimen"
        android:layout_height="@dimen/progressBar_dimen_height"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@+id/steps_progressBar"
        app:layout_constraintTop_toTopOf="parent"
        app:progress="35"
        app:progressBarThickness="4dp"
        android:clickable="true"
        android:focusable="true" />

</androidx.constraintlayout.widget.ConstraintLayout>
